    <text xml:base="urn:cts:croala:anonymus_1020.croala747233.croala-lat1:" xml:lang="lat" type="poesis">
        <body xml:lang="lat" n="body1">
         <div n="div1" type="poesis-inscriptio" met="iambicum-dimetrum">
    
            <l n="l1">O princeps Petre principum,</l>
            <l n="l2">caelestis aulae claviger</l>
            <l n="l3">Devotionis suscipe</l>
            <l n="l4">munusculum quod voveram</l>
            <l n="l5">Proconsul ego infimus</l>
            <l n="l6">Gregorius qui nominor</l>
            <l n="l7">Ut pie mihi conferas</l>
            <l n="l8">pro parvis magna munera</l>
         </div>    
                    </body>

 

