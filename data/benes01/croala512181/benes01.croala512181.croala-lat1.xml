   <text xml:base="urn:cts:croala:benes01.croala512181.croala-lat1:" xml:lang="lat">
      <body xml:lang="lat">
         <div type="poesis-epigramma" met="hendecasyllabus">
            <head n="head">Ad Alesum Abbatem</head>
            <l n="l1">        Quotquot littore uel fuere nostro,</l>
             
            <l n="l2">        Vel sunt, aut aliis erunt in annis,</l>
             
            <l n="l3">        Cunctarum pater eruditionum,</l>
             
            <l n="l4">        Et qui quotidie referre pergis</l>
             
            <l n="l5">        Quantos Illyricum priore saeclo</l>
             
            <l n="l6">        Nec nouisse latus puto lepores.</l>
             
            <l n="l7">        Istae sic oleae otium perenne</l>
             
            <l n="l8">        Componant tibi, laeserint nec Austri</l>
             
            <l n="l9">        Vicinum nemus, ut queat trapetis</l>
             
            <l n="l10">        Felix solvere debitum laboris.</l>
             
            <l n="l11">        Neu celes modo de meis Camoenis</l>
             
            <l n="l12">        Quid, doctissime, senseris: Catullum</l>
             
            <l n="l13">        An quidquam redolent, Horatiumue?</l>
             
            <l n="l14">        Mallem hoc, quam Lybias, utrumque et Indum.</l>
             
            </div>
      </body>

