   <text xml:base="urn:cts:croala:palikuc01.croala1332110.croala-lat1:" xml:lang="lat">
      <body n="body1">
         <milestone unit="page" n="milestone1"/>
         <gap n="gap2" reason="non-necesse"/>
         <div n="div3" met="elegiacum" type="poesis-epigramma">
            <head n="head1">EIVSDEM IN SVAM PATRIAM ENCOMIASTICON.</head>

            <l n="l2">Quae iacet Illyricis Elaphite nobilis undis</l>
             
            <l n="l3" met="pentameter">Insula, Racusio subditur imperio.</l>
             
            <l n="l4">Gignit parua viros, melius queis gurgite vasto</l>
             
            <l n="l5" met="pentameter">Nulla per aequoreum gens trabe currit iter.</l>
             
            <l n="l6">Hic nemus Hesperidum, sunt hic Peneia Tempe;</l>
             
            <l n="l7" met="pentameter">Sunt fontis latices, Naiadesque Deae</l>
             
            <l n="l8">Neptunus fert sceptra, Venus Cytherea videtur</l>
             
            <l n="l9" met="pentameter">Post habuisse Gnidon, postque habuisse Cypron.</l>
             
            <l n="l10">I nunc, qui blateras, in nos mendacia scribe;</l>
             
            <l n="l11" met="pentameter">Dentes ni cesses experiere meos.</l>
             
            <gap n="gap12" reason="non-necesse"/>
         </div>
      </body>
   </text>
