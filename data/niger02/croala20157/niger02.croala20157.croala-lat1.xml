  <text xml:base="urn:cts:croala:niger02.croala20157.croala-lat1:" xml:lang="lat">
      <body n="body1">
         <pb n="pb1"/>
         <div n="div2" type="poesis-epigramma" met="elegiacum">
            <head n="head1">
               <persName n="persName1">Thomae Nigri Dalmatae</persName>, protonotarii Apostolici, 
               in piam curam <persName n="persName2">Ioannis Statilii Traguritae</persName>
            </head>
            <l n="l2">Hoc tibi, diue pater praesul, uenerande <persName n="persName1">Ioannes</persName>,</l>
             
            <l n="l3" met="pentameter">            Statilius propriis sumptibus optat opus,</l>
             
            <l n="l4">            Vt tua quae fuerint miracula facta <placeName n="placeName1">Traguri</placeName>
            </l>
             
            <l n="l5" met="pentameter">            Hic liber excussus testis in orbe foret.</l>
             
            <l n="l6">            Tu miranda facis, facit hic ea publica cunctis,</l>
             
            <l n="l7" met="pentameter">            Laude manet merito dignus uterque sua.</l>
             
            <l n="l8">            Nec minus ipse tuo debes, pie praesul, alumno,</l>
             
            <l n="l9" met="pentameter">            Quam tibi <persName n="persName1">Moeonides</persName> debet <persName n="persName2">Achilles</persName> heros.</l>
             
            
                
         </div>
      </body>
  </text>
