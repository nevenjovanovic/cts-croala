      <body n="body1">
         <pb n="pb1"/>
         <div n="div2" type="poesis-epigramma" met="elegiacum">
            <head n="head1">
               <add n="add1">XXXVII</add> 
               <expan n="expan2">Franciscus Natalis</expan>
            </head>
            <l n="l2">Plurima <corr n="corr1">Callymachi</corr> venerantur saecula manes</l>
             
            <l n="l3" met="pentameter">Scriptaque Nasonis dulcia<ref n="ref1">2</ref> ad Astra ferunt,</l>
             
            <l n="l4">Est qui Pyndareos uario modulamine uersus</l>
             
            <l n="l5" met="pentameter">Subleuat, hic carmen, docte<ref n="ref1">3</ref> Catulle, tuum;</l>
             
            <l n="l6">Vatibus iis tantis ausim praeponere Paulum,</l>
             
            <l n="l7" met="pentameter">Temporibus nostris te, Paladine, sacrum.</l>
             
            <closer n="closer8">
               <foreign n="foreign1" xml:lang="grc">ΤΕΛΟΣ</foreign>
            </closer>
