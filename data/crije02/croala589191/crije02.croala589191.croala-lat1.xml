   <text xml:base="urn:cts:croala:crije02.croala589191.croala-lat1:" xml:lang="lat">
      <body xml:lang="lat" n="body1">
         <milestone unit="page" n="milestone1"/>
         <pb n="pb2"/>
         <div n="div3" type="poesis-epigramma" met="elegiacum">
            <head n="head1">
               <add n="add1">XXXV</add> Aelius Lampridius Cervinus Paulo Palladino suo</head>
            <l n="l2"> Ignigenae<ref n="ref1">2</ref> seu te Bachi furor entheus<ref n="ref2">3</ref> urget</l>
            <l n="l3" met="pentameter"> Concitat aut sensus aenuchia<ref n="ref1">4</ref> flamma tuos,</l>
            <l n="l4"> Seu te Pieriis reptantem, Paule, sub antris</l>
            <l n="l5" met="pentameter"> Nutriit Aonii<ref n="ref1">5</ref> docta puella gregis,</l>
            <l n="l6"> Seu tibi Castalias<ref n="ref1">6</ref> largitur Cynthius<ref n="ref2">7</ref> artes,</l>
            <l n="l7" met="pentameter"> Temperat et docto plectra Magisterio,</l>
            <l n="l8"> Eya age, nec tanta desistas indole foelix</l>
            <l n="l9" met="pentameter"> Musarum et solitas ingrediare uias.</l>
            <l n="l10"> Phoebo redde Linon<ref n="ref1">8</ref>, lacrymosaque fata rependas </l>
            <l n="l11" met="pentameter"> Orphea<ref n="ref1">9</ref> Calyopen <corr n="corr2">dedidicisse</corr>
               <ref n="ref3">10</ref> face<ref n="ref4">11</ref>.</l>
            <closer n="closer12">
               <foreign n="foreign1" xml:lang="grc">ΤΕΛΟΣ</foreign>
            </closer>
         </div>
               </body>
   </text>
